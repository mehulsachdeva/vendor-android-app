<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.user.dailyveg">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:hardwareAccelerated="false"
        android:largeHeap="true"
        android:theme="@style/AppTheme">

        <activity android:name=".HomeActivity" android:theme="@style/Theme.AppCompat.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".MainActivity" android:noHistory="true">
        </activity>

        <activity android:name=".MainActivity2" android:noHistory="true">
        </activity>

        <activity android:name=".MainActivity3" android:noHistory="true">
        </activity>

        <activity android:name=".VendorPriceUpdate" android:noHistory="true">
        </activity>

        <activity android:name=".OrderHistoryDisplay" android:noHistory="true">
        </activity>

        <activity android:name=".PaymentActivity" android:noHistory="true">
        </activity>

    </application>

</manifest>